задние номер 15 на языке си 
#include <stdio.h>

int main() {
    int count = 0;
    double number;

    while (count < 10) {
        printf("Введите число: ");
        scanf("%lf", &number);
        
        if (number * number < 100) {
            printf("Число: %.2f, Квадрат: %.2f\n", number, number * number);
        }
        
        count++;
    }

    return 0;
}
